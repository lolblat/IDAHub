    "http-errors": "~1.6.2",
    "jade": "~1.11.0",
    "mongoose": "^5.5.3",
    "morgan": "~1.9.0",
    "rimraf": "^2.6.3"
  },
  "devDependencies": {
    "nodemon": "^1.18.11"
  }
}
